{
    "name": "xaraya/properties",
    "type": "xaraya-bundle",
    "description": "Standalone properties to extend Xaraya's functionality",
    "license": "GPL-2.0-or-later",
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    },
    "require": {},
    "suggest": {},
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "xaraya/properties": "source"
        }
    },
    "autoload": {
        "classmap": ["./"],
        "exclude-from-classmap": ["/vendor/"]
    }
}
